    "registry": "https://npm.pkg.github.com/sweetgreen"
  },
  "version": "1.7.6",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "description": "SG node logger",
  "repository": "git@github.com:sweetgreen/sg-node-logger.git",
  "author": "Unknown Kale",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/dotenv": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cz-conventional-changelog": "^3.3.0",
    "danger": "^11.1.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.1.2",
    "semantic-release": "^19.0.5",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.171.0",
    "@types/node": "^18.7.18",
    "@types/winston": "^2.4.4",
    "aws-sdk": "^2.1216.0",
    "dotenv": "^16.0.2",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^6.1.1"
  },
  "files": [
    "lib/**/*",
    "CHANGELOG.md",
    "package.json",
    "yarn.lock"
  ],
  "scripts": {
    "start": "npx tsc && node -r dotenv/config lib/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "cz": "cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
