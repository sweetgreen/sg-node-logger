{
  "name": "@sweetgreen/sg-node-logger",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/sweetgreen"
  },
  "version": "1.6.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "description": "SG node logger",
  "repository": "git@github.com:sweetgreen/sg-node-logger.git",
  "author": "Unknown Kale",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/dotenv": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cz-conventional-changelog": "^3.3.0",
    "danger": "^10.5.4",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "semantic-release": "^17.2.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/node": "^14.14.6",
    "@types/winston": "^2.4.4",
    "aws-sdk": "^2.798.0",
    "dotenv": "^8.2.0",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^2.4.0"
  },
  "files": [
    "lib/**/*",
    "CHANGELOG.md",
    "package.json",
    "yarn.lock"
  ],
  "scripts": {
    "start": "npx tsc && node -r dotenv/config lib/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
